DB_CONNECTION_STRING={ The MongoDB connection string. }
PORT={ The port that the server listens for after connections. }

PORT=8080
BASE_URL="/"

SESSION_NAME={ "MakeARandomString" }
SESSION_SECRET={ "MakeARandomString" }

APP_ID = { A serial letters and numbers found in the OAuth service. }
APP_SECRET = { A serial letters and numbers found in the OAuth service. }
REDIRECT_URI = "http://localhost:8080/users/callback"
AUTH_URI = { A link to OAuth service. }
REQUESTED_SCOPE = "read_api+read_user+read_registry+openid+profile+email"